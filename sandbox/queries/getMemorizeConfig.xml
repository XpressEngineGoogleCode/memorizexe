<query id="getMemorizeConfig" action="select">
	<tables>
		<table name="memorize_config" />
	</tables>
	<columns>
		<column name="config" />
	</columns>
	<conditions>
		<condition operation="equal" column="module" var="module" notnull="notnull" pipe="where" />
		<condition operation="equal" column="module_srl" var="module_srl" filter="number" notnull="notnull" pipe="and" />
	</conditions>
</query>